{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "vite",
        "type": "npm",
        "script": "dev:env",
        "isBackground": true,
        "problemMatcher": {
          "owner": "custom",
          "pattern": {
            "regexp": ".",
            "file": 1,
            "location": 2,
            "message": 3
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "running",
            "endsPattern": "Compiled|Failed|Ready|compiled|failed|ready"
          }
        }
      }
    ]
  }
